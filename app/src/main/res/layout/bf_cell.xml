                <!--android:layout_height="20dp"-->
                <!--android:textSize="5pt"-->
                <!--android:textColor="@color/cwnu_main_color9"-->
                <!--android:text="별로에요 수"-->
                <!--android:id="@+id/bf_cell_bad_count_textview"/>-->
        </LinearLayout>



    </LinearLayout>





